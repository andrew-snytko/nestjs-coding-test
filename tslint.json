{
    "defaultSeverity": "error",
    "extends": [
        "tslint:latest",
        "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {
        "no-console": false,
        "interface-name": false,
        "object-literal-sort-keys": false,
        "max-classes-per-file": false,
        "member-access": true,
        "ordered-imports": true
    },
    "rulesDirectory": [],
    "linterOptions": {
        "exclude": [
            "src/migrations/*.ts",
            "*.md"
        ]
    }
}